{
  "name": "-webkit-nbsp-mode",
  "description": "Defines the behavior of nonbreaking spaces within text.",
  "keywords": [
    "css",
    "property",
    "-webkit-nbsp-mode",
    "webkit",
    "nbsp",
    "mode"
  ],
  "longDescription": "",
  "syntax": [
    "-webkit-nbsp-mode: normal | space;"
  ],
  "defaultValue": "normal",
  "values": {
